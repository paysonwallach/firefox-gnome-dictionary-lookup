{
  "manifest_version": 2,
  "name": "GNOME Dictionary Lookup",
  "description": "Lookup words in GNOME Dictionary",
  "version": "0.1.0",
  "background": {
    "scripts": ["lookup.js"]
  },
  "icons": {
    "16": "icon.svg",
    "24": "icon.svg",
    "32": "icon.svg",
    "48": "icon.svg",
    "64": "icon.svg",
    "96": "icon.svg",
    "128": "icon.svg"
  },
  "permissions": ["contextMenus", "nativeMessaging"],
  "applications": {
    "gecko": {
      "id": "gnome-dictionary-lookup@paysonwallach.com"
    }
  }
}
